`timescale 1 ps/ 1 ps
module cw3_vlg_sample_tst(
	clk,
	DOWN,
	UP,
	sampler_tx
);
input  clk;
input  DOWN;
input  UP;
output sampler_tx;

reg sample;
time current_time;
always @(clk or DOWN or UP)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

