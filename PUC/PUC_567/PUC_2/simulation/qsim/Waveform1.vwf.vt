reg clk;
reg DOWN;
reg UP;
// wires                                               
wire [3:0] bcd0;
wire [3:0] bcd1;
wire [7:0] LED0;
wire [7:0] LED1;
wire LEDG9;

wire sampler;                             

// assign statements (if any)                          
cw3 i1 (
// port map - connection between master ports and signals/registers   
	.bcd0(bcd0),
	.bcd1(bcd1),
	.clk(clk),
	.DOWN(DOWN),
	.LED0(LED0),
	.LED1(LED1),
	.LEDG9(LEDG9),
	.UP(UP)
);

// clk
initial
begin
	repeat(166)
	begin
		clk = 1'b0;
		clk = #300000 1'b1;
		# 300000;
	end
	clk = 1'b0;
	clk = #300000 1'b1;
end 

// UP
initial
begin
	UP = 1'b1;
	UP = #2560000 1'b0;
	UP = #1920000 1'b1;
	UP = #1920000 1'b0;
	UP = #1920000 1'b1;
	UP = #2560000 1'b0;
	UP = #1920000 1'b1;
	UP = #1280000 1'b0;
	UP = #10240000 1'b1;
	UP = #3840000 1'b0;
	UP = #2560000 1'b1;
	UP = #3840000 1'b0;
	UP = #21760000 1'b1;
	UP = #3200000 1'b0;
end 

// DOWN
initial
begin
	DOWN = 1'b0;
	DOWN = #14720000 1'b1;
	DOWN = #2560000 1'b0;
	DOWN = #1920000 1'b1;
	DOWN = #3200000 1'b0;
	DOWN = #1920000 1'b1;
	DOWN = #3840000 1'b0;
	DOWN = #7040000 1'b1;
	DOWN = #1920000 1'b0;
	DOWN = #1920000 1'b1;
	DOWN = #1920000 1'b0;
	DOWN = #1280000 1'b1;
	DOWN = #1920000 1'b0;
	DOWN = #1920000 1'b1;
	DOWN = #1920000 1'b0;
	DOWN = #1920000 1'b1;
	DOWN = #2560000 1'b0;
	DOWN = #1280000 1'b1;
	DOWN = #1920000 1'b0;
end 

cw3_vlg_sample_tst tb_sample (
	.clk(clk),
	.DOWN(DOWN),
	.UP(UP),
	.sampler_tx(sampler)
);

cw3_vlg_check_tst tb_out(
	.bcd0(bcd0),
	.bcd1(bcd1),
	.LED0(LED0),
	.LED1(LED1),
	.LEDG9(LEDG9),
	.sampler_rx(sampler)
);
endmodule

